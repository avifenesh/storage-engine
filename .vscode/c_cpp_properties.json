{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_POSIX_C_SOURCE=200809L",
                "_GNU_SOURCE",
                "CONFIG_ARM64",
                "CONFIG_64BIT",
                "CONFIG_AS_CFI=1",
                "CONFIG_AS_CFI_SIGNAL_FRAME=1",
                "CONFIG_AS_CFI_SECTIONS=1"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm64",
            "compilerArgs": [
                "-pthread"
            ]
        }
    ],
    "version": 4
}
